Host github.com
  User beejei.public
  AddKeysToAgent yes
  IgnoreUnknown UseKeychain
  UseKeychain yes
  IdentityFile ~/.ssh/id_ed25519_github.com
  StrictHostKeyChecking no

Host github.my.internal
  User beejei.internal
  AddKeysToAgent yes
  IgnoreUnknown UseKeychain
  UseKeychain yes
  IdentityFile ~/.ssh/id_ed25519_github.my.internal
  StrictHostKeyChecking no

